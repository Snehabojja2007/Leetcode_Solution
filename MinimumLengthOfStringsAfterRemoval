class Solution {
    public int minLengthAfterRemovals(String s) {
        int n=s.length();
        int count=0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='a'){
                count++;
            }else{
                count--;
            }
        }
        return Math.abs(count);
    }
}
